{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "quotes": [2, "single"],
    "import/extensions": [0],
    "import/no-unresolved": [0],
    "no-underscore-dangle": [0],
    "consistent-return": [0],
    "linebreak-style": 0
  }
}
